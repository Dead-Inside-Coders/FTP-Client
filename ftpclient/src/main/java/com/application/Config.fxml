<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="405.0" prefWidth="607.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.application.Controller">
   <children>
      <ListView fx:id="listFiles" focusTraversable="false" layoutX="23.0" layoutY="104.0" prefHeight="238.0" prefWidth="575.0" />
      <Button fx:id="showButton" layoutX="23.0" layoutY="40.0" mnemonicParsing="false" onAction="#showFiles" prefHeight="49.0" prefWidth="114.0" text="Показать файлы" />
      <Button fx:id="downloadButton" layoutX="23.0" layoutY="352.0" mnemonicParsing="false" onAction="#downloadSelectedFile" prefHeight="39.0" prefWidth="106.0" text="Скачать файл" />
      <Button fx:id="logsButton" layoutX="506.0" layoutY="352.0" mnemonicParsing="false" onAction="#getLogs" prefHeight="39.0" prefWidth="92.0" text="Логи" />
      <Button fx:id="uploadButton" layoutX="137.0" layoutY="352.0" mnemonicParsing="false" onAction="#uploadFileToServer" prefHeight="39.0" prefWidth="159.0" text="Загрузить файл на сервер" visible="false" />
      <Button fx:id="DisconnectId" layoutX="509.0" layoutY="21.0" mnemonicParsing="false" onAction="#disconnecting" prefHeight="39.0" prefWidth="86.0" text="Disconnect" />
      <Button fx:id="delButton" layoutX="418.0" layoutY="352.0" mnemonicParsing="false" onAction="#deleteFile" prefHeight="39.0" prefWidth="78.0" text="Удалить" visible="false" />
      <Button fx:id="renameButton" layoutX="304.0" layoutY="352.0" mnemonicParsing="false" onAction="#renameFile" prefHeight="39.0" prefWidth="106.0" text="Переименовать" visible="false" />
   </children>
</AnchorPane>
