<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="420.0" prefWidth="617.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.application.Controller">
   <children>
      <ListView fx:id="listFiles" focusTraversable="false" layoutX="52.0" layoutY="118.0" prefHeight="224.0" prefWidth="526.0" />
      <Button fx:id="showButton" layoutX="48.0" layoutY="51.0" mnemonicParsing="false" onAction="#showFiles" prefHeight="49.0" prefWidth="114.0" text="Показать файлы" />
      <Button fx:id="downloadButton" layoutX="52.0" layoutY="365.0" mnemonicParsing="false" onAction="#downloadSelectedFile" prefHeight="39.0" prefWidth="106.0" text="Скачать файл" visible="false" />
      <Button fx:id="logsButton" layoutX="486.0" layoutY="362.0" mnemonicParsing="false" onAction="#getLogs" prefHeight="39.0" prefWidth="92.0" text="Логи" />
      <Button fx:id="uploadButton" layoutX="195.0" layoutY="365.0" mnemonicParsing="false" onAction="#uploadFileToServer" prefHeight="39.0" prefWidth="171.0" text="Загрузить файл на сервер" visible="false" />
      <Button fx:id="DisconnectId" layoutX="525.0" layoutY="14.0" mnemonicParsing="false" onAction="#disconnecting" prefHeight="25.0" prefWidth="78.0" text="disconnect" />
      <Button fx:id="delButton" layoutX="400.0" layoutY="362.0" mnemonicParsing="false" onAction="#deleteFile" prefHeight="39.0" prefWidth="78.0" text="Удалить" visible="false" />
   </children>
</AnchorPane>
